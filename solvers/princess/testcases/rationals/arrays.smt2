; We should be able to decide constraints in the quantifier-free
; combination of arrays and rationals (and strings)

(declare-fun Constructed_Argument_t () String)
(declare-fun CreateAsClass () Bool)
(declare-fun Constructed_Argument () Bool)
(declare-fun ExpandObjSwitcher () Real)
(declare-fun Constructed_Argument_2_t () String)
(declare-fun Constructed_Argument_2_Array_Length () Int)
(declare-fun CreateAsClass_2 () Bool)
(declare-fun Constructed_Argument_2 () (Array Int String))
(assert (= Constructed_Argument_t "boolean"))
(assert (not CreateAsClass))
(assert (not Constructed_Argument))
(assert (= ExpandObjSwitcher 0.0))
(assert (= Constructed_Argument_2_t "array_string"))
(assert (>= Constructed_Argument_2_Array_Length 0))
(assert (not CreateAsClass_2))
(assert (not (<= (to_real Constructed_Argument_2_Array_Length) 0.0)))
(assert (not (= (select Constructed_Argument_2 0) "")))
(assert (not (<= (to_real Constructed_Argument_2_Array_Length) 1.0)))
(check-sat)
