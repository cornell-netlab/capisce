(set-logic AUFLIA)

(declare-fun f (Int) Int)
(declare-fun h (Int) Int)
(declare-fun |h'| (Int) Int)
(declare-fun a () Int)
(declare-fun b () Int)
(declare-fun c () Int)
(declare-fun n () Int)
(declare-fun i () Int)
(declare-fun j () Int)

(assert (forall ((x1 Int) (x2 Int))
                (=> (<= 0 x1)
                (=> (<= x1 x2)
                (=> (<= x2 n)
                    (<= (h (f x1)) (h (f x2))))))))
(assert (forall ((x1 Int))
                (=> (<= 0 x1)
                (=> (<= x1 n)
                    (not (= (f x1) a))))))
(assert (forall ((x1 Int))
                (=> (not (= x1 a))
                    (= (|h'| x1) (h x1)))))
(assert (= (|h'| a) b))
(assert (<= 0 i))
(assert (<= i j))
(assert (<= j n))
(assert (> (|h'| (f i)) (|h'| (f j))))

(check-sat)
(exit)